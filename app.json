{
  "expo": {
    "name": "Сойдёт",
    "slug": "horosho",
    "version": "1.3.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "horosho",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "description": "Приложение для изучения и практики навыков диалектической поведенческой терапии (DBT). Включает теоретические материалы, дневник эмоций и инструменты для развития эмоциональной регуляции.",
    "privacy": "public",
    "primaryColor": "#2d2d4a",
    "privacyPolicyUrl": "https://mitenka.github.io/horosho/privacy-policy.html",
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "app.mitenka.sojdet",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "app.mitenka.sojdet"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 240,
          "imageHeight": 240,
          "resizeMode": "contain",
          "backgroundColor": "#2d2d4a"
        }
      ],
      "expo-asset",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "ee8a5094-2533-4160-9886-1461da545e4d"
      }
    }
  }
}
